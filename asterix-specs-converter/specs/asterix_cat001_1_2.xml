                        <BitsValue val="5">Second time around reply</BitsValue>
                        <BitsValue val="6">Angels</BitsValue>
                        <BitsValue val="7">Terrestrial vehicles</BitsValue>
                        <BitsValue val="64">Possible wrong code in Mode-3/A</BitsValue>
                        <BitsValue val="65">Possible wrong altitude information, transmitted when the Code C credibility check fails together with the Mode-C code in binary notation</BitsValue>
                        <BitsValue val="66">Possible phantom MSSR plot</BitsValue>
                        <BitsValue val="80">Fixed PSR plot</BitsValue>
                        <BitsValue val="81">Slow PSR plot</BitsValue>
                        <BitsValue val="82">Low quality PSR plot</BitsValue>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
