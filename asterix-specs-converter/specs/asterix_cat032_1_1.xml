    <DataItem id="400">
        <DataItemName>Callsign</DataItemName>
        <DataItemDefinition>
            Callsign (in 7 characters) of an aircraft (provided in the Miniplan).
        </DataItemDefinition>
        <DataItemFormat desc="7-octet fixed length data item.">
            <Fixed length="7">
                <Bits from="56" to="1" encode="ascii">
                    <BitsShortName>C</BitsShortName>
                    <BitsName>Callsign</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - Each one of the seven octets contains an ASCII Character. The Callsign
                  is always left adjusted. It contains up to seven upper-case alphanumeric
                  characters, the remaining character positions (if any) are padded with
                  space characters.
        </DataItemNote>
    </DataItem>

