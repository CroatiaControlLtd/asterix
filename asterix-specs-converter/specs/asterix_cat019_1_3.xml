                </Bits>
                <Bits from="32" to="1" encode="signed">
                    <BitsShortName>LON</BitsShortName>
                    <BitsName>Longitude</BitsName>
                    <BitsUnit scale="0.0000001676380634307861328125" min="-180" max="180">deg</BitsUnit>
