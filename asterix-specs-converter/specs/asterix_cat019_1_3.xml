    <DataItem id="610">
        <DataItemName>Height of the MLT System Reference Point</DataItemName>
        <DataItemDefinition>
            Height of the MLT system reference point in two&apos;s complement form.
            The height shall use mean sea level as the zero reference level.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>HotMSRP</BitsShortName>
                    <BitsName>Height of the MLT System Reference Point</BitsName>
                    <BitsUnit scale="0.25" min="-8192" max="8192">m</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="620">
        <DataItemName>WGS-84 Undulation</DataItemName>
        <DataItemDefinition>
            WGS-84 undulation value of the MLT system reference point, in meters.
