    <DataItem id="021">
        <DataItemName>Broadcast Properties</DataItemName>
        <DataItemDefinition>
            Properties of an uplink broadcast request (power, duration, coverage).
        </DataItemDefinition>
        <DataItemFormat desc="6-octet fixed length data item.">
            <Fixed length="6">
                <Bits from="48" to="45" encode="unsigned">
                    <BitsShortName>PRIORITY</BitsShortName>
                    <BitsName>Priority</BitsName>
                    <BitsUnit></BitsUnit>
                </Bits>
                <Bits from="44" to="41" encode="unsigned">
                    <BitsShortName>POWER</BitsShortName>
                    <BitsName>Power</BitsName>
                    <BitsUnit></BitsUnit>
                </Bits>
                <Bits from="40" to="33" encode="unsigned">
                    <BitsShortName>DURATION</BitsShortName>
                    <BitsName>Duration</BitsName>
                    <BitsUnit scale="1.0">s</BitsUnit>
                </Bits>
                <Bits from="32" to="1">
                    <BitsShortName>COVERAGE</BitsShortName>
                    <BitsName>Coverage</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Notes:
                 1. A broadcast with a higher priority will temporarily delay a lower priority broadcast if
                    necessary. The delayed broadcast will be resumed as soon as possible for its
                    remaining time.
                 2. The sectors are numbered from 1 to 32 clockwise, sector 1 being the first sector after the North.
        </DataItemNote>
    </DataItem>

    <DataItem id="022">
        <DataItemName>Broadcast Prefix</DataItemName>
        <DataItemDefinition>
            Contents of the 32 first bits of an uplink broadcast interrogation.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="28">
                    <BitsShortName>spare</BitsShortName>
                    <BitsName>Spare bit(s) set to 0</BitsName>
                    <BitsConst>0</BitsConst>
                </Bits>
                <Bits from="27" to="1">
                    <BitsShortName>PREFIX</BitsShortName>
                    <BitsName>Prefix Field</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - The Mode S uplink broadcast interrogation will be made up of this
                  D_Broadcast_prefix field followed by the D_Broadcast field and then by the
                  Address/Parity field (in this order), as defined in ICAO Annex 10. In the
                  interrogator, the 5 first bits of D_Broadcast_prefix will be replaced by ICAO
                  UF field, as defined in Annex 10.
        </DataItemNote>
    </DataItem>

    <DataItem id="023">
        <DataItemName>Uplink or Downlink Broadcast</DataItemName>
        <DataItemDefinition>
            Broadcast message sent (MA field of the Comm-A frame) or received
            (MB field of the Comm-B frame), conformant with the ICAO Manual on
            Mode S Specific Services.
        </DataItemDefinition>
        <DataItemFormat desc="7-octet fixed length data item.">
            <Fixed length="7">
                <Bits from="56" to="1">
                    <BitsShortName>UoDB</BitsShortName>
                    <BitsName>Uplink or Downlink Broadcast</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="025">
        <DataItemName>GICB Number</DataItemName>
        <DataItemDefinition>
            Number used to correlate subsequent GICB messages (i.e. responses
            and acknowledgements) with the original GICB request.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="1" encode="unsigned">
                    <BitsShortName>GN</BitsShortName>
                    <BitsName>GICB Number</BitsName>
                    <BitsUnit></BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="027">
        <DataItemName>BDS Code</DataItemName>
        <DataItemDefinition>
            BDS code of the GICB to be extracted.
        </DataItemDefinition>
        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits from="8" to="1">
                    <BitsShortName>BC</BitsShortName>
                    <BitsName>BDS Code</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="028">
        <DataItemName>GICB Extraction Periodicity</DataItemName>
        <DataItemDefinition>
            Periodicity of the GICB extractions.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="unsigned">
                    <BitsShortName>GEP</BitsShortName>
                    <BitsName>GICB Extraction Periodicity</BitsName>
                    <BitsUnit scale="1.0">s</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="029">
        <DataItemName>GICB Extracted</DataItemName>
        <DataItemDefinition>
            GICB extracted message (MB field of the Comm-B frame). i.e., the
            contents of a BDS register, conformant with the ICAO Manual on
            Mode S Specific Services.
        </DataItemDefinition>
        <DataItemFormat desc="7-octet fixed length data item.">
            <BDS/>
        </DataItemFormat>
    </DataItem>

    <DataItem id="030">
        <DataItemName>GICB Properties</DataItemName>
        <DataItemDefinition>
            Properties of the GICB extractions.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="12" encode="unsigned">
                    <BitsShortName>PRIORITY</BitsShortName>
                    <BitsName>GICB Priority</BitsName>
                    <BitsUnit></BitsUnit>
                </Bits>
                <Bits from="11" to="9">
                    <BitsShortName>spare</BitsShortName>
                    <BitsName>Spare bit(s) set to 0</BitsName>
                    <BitsConst>0</BitsConst>
                </Bits>
                <Bits bit="8">
                    <BitsShortName>PC</BitsShortName>
                    <BitsName>Periodicity Constraint</BitsName>
                    <BitsValue val="0">The periodicity may not be strictly respected</BitsValue>
                    <BitsValue val="1">The periodicity shall be strictly respected</BitsValue>
                </Bits>
                <Bits bit="7">
                    <BitsShortName>AU</BitsShortName>
                    <BitsName>Asynchronous Update</BitsName>
                    <BitsValue val="0">GICB extractions should be sent only when required by the periodicity</BitsValue>
                    <BitsValue val="1">If a GICB extraction is done due to external conditions, an update will also be sent, even if it does not match the expected periodicity</BitsValue>
                </Bits>
                <Bits bit="6">
                    <BitsShortName>NE</BitsShortName>
                    <BitsName>Non Extraction</BitsName>
                    <BitsValue val="0">The GICB extraction is attempted according to the periodicity</BitsValue>
                    <BitsValue val="1">There will no GICB attempts</BitsValue>
                </Bits>
                <Bits from="5" to="4">
                    <BitsShortName>RD</BitsShortName>
                    <BitsName>Reply Destination</BitsName>
                    <BitsValue val="0">The extracted GICB must be sent only on the Data Link line</BitsValue>
                    <BitsValue val="1">The extracted GICB must be sent only on the Surveillance line</BitsValue>
                    <BitsValue val="2">The extracted GICB must be sent both on the Data Link and on the Surveillance lines</BitsValue>
                </Bits>
                <Bits from="3" to="1">
                    <BitsShortName>spare</BitsShortName>
                    <BitsName>Spare bit(s) set to 0</BitsName>
                    <BitsConst>0</BitsConst>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - The Non Extraction flag (NE) should be used only if the Asynchronous
                  Update flag (AU) is set to true. It is specially reserved to the ACAS&apos; RA
                  extraction (asynchronous update without periodic extraction request).
        </DataItemNote>
    </DataItem>

    <DataItem id="031">
        <DataItemName>Aircraft Identity</DataItemName>
        <DataItemDefinition>
            Identity of the aircraft extracted by a BDS 20 as described in ICAO Annex 10.
        </DataItemDefinition>
        <DataItemFormat desc="6-octet fixed length data item.">
            <Fixed length="6">
                <Bits from="48" to="1">
                    <BitsShortName>AI</BitsShortName>
                    <BitsName>Aircraft Identity</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="032">
        <DataItemName>Aircraft Mode A</DataItemName>
        <DataItemDefinition>
            Mode-3/A code converted into octal representation.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits bit="16">
                    <BitsShortName>V</BitsShortName>
                    <BitsValue val="0">Code validated</BitsValue>
                    <BitsValue val="1">Code not validated</BitsValue>
                </Bits>
                <Bits bit="15">
                    <BitsShortName>G</BitsShortName>
                    <BitsValue val="0">Default</BitsValue>
                    <BitsValue val="1">Garbled code</BitsValue>
                </Bits>
                <Bits bit="14">
                    <BitsShortName>L</BitsShortName>
                    <BitsValue val="0">Mode-3/A code derived from the reply of the transponder</BitsValue>
                    <BitsValue val="1">Mode-3/A code not extracted during the last scan</BitsValue>
                </Bits>
                <Bits bit="13">
                    <BitsShortName>spare</BitsShortName>
                    <BitsName>Spare bit(s) set to 0</BitsName>
                    <BitsConst>0</BitsConst>
                </Bits>
                <Bits from="12" to="1" encode="octal">
                    <BitsShortName>MOD3A</BitsShortName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - Bit 15 has no meaning in the case of a smoothed Mode-3/A code and is set
                  to 0 for a calculated track. For Mode S, it is set to one when an error
                  correction has been attempted.
        </DataItemNote>
    </DataItem>

    <DataItem id="033">
        <DataItemName>Aircraft Height</DataItemName>
        <DataItemDefinition>
            Flight Level converted into binary representation.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits bit="16">
                    <BitsShortName>V</BitsShortName>
                    <BitsValue val="0">Code validated</BitsValue>
                    <BitsValue val="1">Code not validated</BitsValue>
                </Bits>
                <Bits bit="15">
                    <BitsShortName>G</BitsShortName>
                    <BitsValue val="0">Default</BitsValue>
                    <BitsValue val="1">Garbled code</BitsValue>
                </Bits>
                <Bits from="14" to="1" encode="signed">
                    <BitsShortName>FL</BitsShortName>
                    <BitsName>Flight Level</BitsName>
                    <BitsUnit scale="0.25">FL</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Notes:
                1. The value shall be within the range described by ICAO Annex 10
                2. For Mode S, bit 15 (G) is set to one when an error correction
                   has been attempted.
                3. If Altitude is not extracted on the last scan, it is an implementation
                   issue as to whether Altitude is output from track file, if at all.
        </DataItemNote>
    </DataItem>

    <DataItem id="034">
        <DataItemName>Aircraft Speed</DataItemName>
        <DataItemDefinition>
            Tracker calculated Ground Speed of an aircraft.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="unsigned">
                    <BitsShortName>AS</BitsShortName>
                    <BitsName>Aircraft Speed</BitsName>
                    <BitsUnit scale="0.00006103515625">NM/s</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="035">
        <DataItemName>Aircraft Heading</DataItemName>
        <DataItemDefinition>
            Tracker calculated heading of an aircraft. . The heading is the heading with respect
            to the geographical north at the aircraft position.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="unsigned">
                    <BitsShortName>AH</BitsShortName>
                    <BitsName>Aircraft Heading</BitsName>
                    <BitsUnit scale="0.0054931640625">deg</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="036">
        <DataItemName>Data Source Identifier</DataItemName>
        <DataItemDefinition>
            Identification of the source node for the GDLP/LU data
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="9">
                    <BitsShortName>SAC</BitsShortName>
                    <BitsName>System Area Code</BitsName>
                </Bits>
                <Bits from="8" to="1">
                    <BitsShortName>SIC</BitsShortName>
                    <BitsName>System Identification Code</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - The up-to-date list of SACs is published on the
                  EUROCONTROL Web Site (http://www.eurocontrol.int/asterix).
        </DataItemNote>
    </DataItem>

    <DataItem id="037">
        <DataItemName>Data Destination Identifier</DataItemName>
        <DataItemDefinition>
            Identification of the destination node for the GDLP/LU data.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="9">
                    <BitsShortName>SAC</BitsShortName>
                    <BitsName>System Area Code</BitsName>
                </Bits>
                <Bits from="8" to="1">
                    <BitsShortName>SIC</BitsShortName>
                    <BitsName>System Identification Code</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - The up-to-date list of SACs is published on the
                  EUROCONTROL Web Site (http://www.eurocontrol.int/asterix).
        </DataItemNote>
    </DataItem>

