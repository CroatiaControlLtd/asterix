        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits from="8" to="1">
                    <BitsShortName>ACCM</BitsShortName>
                    <BitsName> Aircraft CQF Calculation Method</BitsName>
