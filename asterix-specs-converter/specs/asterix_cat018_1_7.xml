                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits bit="8">
                        <BitsShortName>ID</BitsShortName>
                        <BitsValue val="0">The next Aircraft_report may not include Aircraft_ID</BitsValue>
                        <BitsValue val="1">The next Aircraft_report shall include Aircraft_ID</BitsValue>
                    </Bits>
                    <Bits bit="7">
                        <BitsShortName>MA</BitsShortName>
                        <BitsValue val="0">The next Aircraft_report may not include Mode_A</BitsValue>
                        <BitsValue val="1">The next Aircraft_report shall include Mode_A</BitsValue>
                    </Bits>
                    <Bits bit="6">
                        <BitsShortName>SP</BitsShortName>
