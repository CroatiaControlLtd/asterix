    <DataItem id="014">
        <DataItemName>Aircraft Position in Polar Co-ordinates</DataItemName>
        <DataItemDefinition>
            Measured position of an aircraft in local polar co-ordinates.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="17" encode="unsigned">
                    <BitsShortName>RHO</BitsShortName>
                    <BitsUnit scale="0.00390625" max="256">NM</BitsUnit>
                </Bits>
                <Bits from="16" to="1" encode="unsigned">
                    <BitsShortName>THETA</BitsShortName>
                    <BitsUnit scale="0.0054931640625">deg</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - When expressed in 16 bits, signed or unsigned azimuths have the same value.
        </DataItemNote>
    </DataItem>

    <DataItem id="015">
        <DataItemName>Aircraft Position in Cartesian Co-ordinates </DataItemName>
        <DataItemDefinition>
            Calculated position of an aircraft in Cartesian co-ordinates.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="17" encode="signed">
                    <BitsShortName>X</BitsShortName>
                    <BitsName>X-Component</BitsName>
                    <BitsUnit scale="0.0078125" min="-256" max="256">NM</BitsUnit>
                </Bits>
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>Y</BitsShortName>
                    <BitsName>Y-Component</BitsName>
                    <BitsUnit scale="0.0078125" min="-256" max="256">NM</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                - Negative values are expressed in 2&apos;s complement form, bit-32 and bit-16
                  shall be set to 0 for positive values and 1 for negative values.
        </DataItemNote>
    </DataItem>

