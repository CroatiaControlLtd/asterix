    <DataItem id="012">
        <DataItemName>Aircraft Coverage Quality Factor</DataItemName>
        <DataItemDefinition>
            Coverage Quality Factor (CQF) of an aircraft (for a given interrogator).
        </DataItemDefinition>
        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits bit="8">
                    <BitsShortName>FS</BitsShortName>
                    <BitsName>Flight Status</BitsName>
                    <BitsValue val="0">Aircraft is airborne</BitsValue>
                    <BitsValue val="1">Aircraft is on the ground</BitsValue>
                </Bits>
                <Bits from="7" to="1">
                    <BitsShortName>CQF</BitsShortName>
