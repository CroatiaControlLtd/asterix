                        <BitsName>Service Status Reporting Period</BitsName>
                        <BitsUnit scale="1.0" min="1" max="127">s</BitsUnit>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
